{
  "name": "monday-integration-quickstart-app-typescript",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "dev-server": "npm run stop && npm run client-build && concurrently \"ts-node-dev --watch ./.env ./src/app.ts\" \"npm run expose\"",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "expose": "mapps tunnel:create -p 8080",
    "stop": "kill-port 8080",
    "deploy": "npm run client-build && mapps code:push",
    "client-build": "cd client && npm run build"
  },
  "dependencies": {
    "@types/node": "^20.14.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.3.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "monday-sdk-js": "^0.5.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.4.5",
    "ngrok": "^5.0.0-beta.2",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "typescript": "^4.1.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^2.1.1",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
